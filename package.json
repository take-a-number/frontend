{
  "name": "frontend",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@blueprintjs/core": "^3.8.0",
    "@blueprintjs/icons": "^3.3.0",
    "@blueprintjs/select": "^3.2.1",
    "@types/react-router-dom": "^4.3.1",
    "history": "^4.7.2",
    "node-sass-chokidar": "^1.3.4",
    "normalize.css": "^8.0.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "yarn-run-all": "^3.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-ts",
    "start-ts": "react-scripts-ts start",
    "build": "npm-run-all -p build-css build-ts",
    "build-ts": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "@types/react": "^16.7.2",
    "@types/react-dom": "^16.0.9",
    "typescript": "^3.1.6"
  }
}
